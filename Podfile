  pod 'Firebase/Storage'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Phone'
  
  #UI
  pod 'Material', '~> 2.0'

  
  target 'iOSFinalProjectTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'iOSFinalProjectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
